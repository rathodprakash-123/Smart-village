  <%- include('partials/header') %>

<div class="slideshow-container" style="max-width:100%; position:relative; margin:auto;">

  <div class="mySlides fade">
    <img src="/images/farm.jpg" style="width:100%; height: 450px; object-fit:cover;">
  </div>

  <div class="mySlides fade">
    <img src="/images/fisherman.jpg" style="width:100%; height: 450px; object-fit:cover;">
  </div>

  <div class="mySlides fade">
    <img src="/images/education.jpg" style="width:100%; height:450px; object-fit:cover;">
  </div>

</div>

<div style="text-align:center; margin-top:10px;">
  <span class="dot"></span> 
  <span class="dot"></span> 
  <span class="dot"></span> 
</div>
<% if(announcements.length !=0) {%>
<div class="announcement-ticker">
  <div class="ticker-track">
    <% announcements.forEach(a => { %>
      <div class="ticker-item">
       <i class="fa-solid fa-bullhorn"></i> <%= a.content %>
      </div>
    <% }) %>
  </div>
</div>
<% } %>

<div class="skills-section">
      <div class="skill-card">
        <i class="fa-solid fa-users"></i>
        <h3>3397+</h3>
        <p>Population</p>
      </div>
      <div class="skill-card">
      <i class="fa-solid fa-eye"></i>
      <h3><%= views %>+</h3>
      <p>Website view</p>
      </div>
      <div class="skill-card">
        <i class="fa-solid fa-map-location-dot"></i>
        <h3>929.81</h3>
        <p>Area hectares</p>
      </div>
</div>
  
  <%- include('partials/footer') %>

